	</screenshots>
	<copyright></copyright>
	<siteLinks>
		<link url="{e_PLUGIN}youtube/youtube.php" sef='index' perm="everyone" >YouTube</link>
	</siteLinks>
	<adminLinks>
		<link url="admin_config.php" icon="images/youtube_32.png" iconSmall="images/youtube_16.png" icon128="images/youtube_128.png" primary="true" >LAN_CONFIGURE</link>
